<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="manifest" href="{{ path('home') }}manifest/{{ render(controller('App\\Controller\\HomeController:environment')) }}.json">
    <link rel="icon" type="image/png" href="https://www.easyamap.fr/images/logo-easy-amap-icon.png" />
    <title>{% block header_title %}{% endblock %} - {{ app_title }} - {{ render(controller('App\\Controller\\HomeController:town')) }} </title>
    <link href="{{  asset('bootstrap/css/bootstrap-yeti.min.css') }}" rel="stylesheet" />
    {% block header_css %}{% endblock %}
    <link href="{{  asset('css/amap.css') }}" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Boogaloo" rel="stylesheet">
    <link href="{{  asset('css/portal.css') }}" rel="stylesheet" />
    <link href="{{  asset('css/print.css') }}" rel="stylesheet" media="print" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/css/select2.min.css" rel="stylesheet" />
    {# include 'Partials/_analytics.html.twig' #} 
  </head>
  <body>
    {% include 'Partials/_header.html.twig' with {'title':  block('header_title')} %}  
      <div class="container">  
      {% include 'Partials/_flashMessage.html.twig' %}      
          
      {% block body %}{% endblock %}  
      </div>
      {{ render(controller('App\\Controller\\HomeController:footer')) }}          
      <script type="text/javascript" src="{{ asset('js/jquery-1.11.2.min.js') }}"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/js/select2.min.js"></script>
{% block footer_js_before %}{# especially for datatable #}{% endblock %}

<script type="text/javascript" src="{{ asset('bootstrap/js/bootstrap.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('js/amap.js') }}"></script>
{% block footer_js %}{% endblock %}
  </body>
</html>
